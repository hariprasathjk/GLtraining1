<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Servlet2</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  
   <servlet>
  <servlet-name>HelloServlet</servlet-name>
   <servlet-class>com.Servlet.basic.BasicServlet</servlet-class>
     </servlet>
   
    <servlet-mapping>
   <servlet-name>HelloServlet</servlet-name>
   <url-pattern>/hello</url-pattern>
   </servlet-mapping>
   
   <servlet>
   <servlet-name>Multiplication</servlet-name>
   <servlet-class>com.Servlet.basic.MultiplyServlet</servlet-class>
     </servlet>
   
    <servlet-mapping>
   <servlet-name>Multiplication</servlet-name>
   <url-pattern>/multiply</url-pattern>
   </servlet-mapping>
   
  <servlet>
   <servlet-name>Addition</servlet-name>
   <servlet-class>com.Servlet.basic.AdditionServlet</servlet-class>
     </servlet>
   
    <servlet-mapping>
   <servlet-name>Addition</servlet-name>
   <url-pattern>/add</url-pattern>
   </servlet-mapping>
   
    <servlet>
   <servlet-name>Subtraction</servlet-name>
   <servlet-class>com.Servlet.basic.SubractionServlet</servlet-class>
     </servlet>
   
    <servlet-mapping>
   <servlet-name>Subtraction</servlet-name>
   <url-pattern>/sub</url-pattern>
   </servlet-mapping>
   
   
   
</web-app>